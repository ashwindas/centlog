<h2>Members List</h2>
<table id="alert_table">
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<% if user != current_user %> <td><%= user.first_name%> <%= user.last_name %></td>
			<td><%= link_to user.email, user %></td>
			<% end %>
		</tr>
		<% end %>
	</tbody>
</table>
